import algorithms.mazeGenerators.Position;
import algorithms.search.BFS;
import algorithms.search.DomainSearchable;
import algorithms.search.Searchable;
import algorithms.search.Searcher;
import algorithms.search.Solution;

public class Run {

	public static void main(String[] args) {
		testSearcher(new BFS<Position>(), new DomainSearchable());
	}

	private static void testSearcher(Searcher<Position> searcher, Searchable searchable) {
		
		Solution<Position> sol = searcher.search(searchable);
		int n = searcher.getNumberOfNodesEvaluated();
	}
}
