#pragma once
#include <string>

class PackageProduct: public virtual Product
{
private:

	const int mAmount;
	string* mProductsNamesList;
	const int mColorCount;

public:

	PackageProduct();
	PackageProduct(int id, int shelf, char row, int weight, ExposureValue exposure,
			int amount, char* productNamesList, int colorcount);
	virtual ~PackageProduct();
	virtual int calculatePrice();
	virtual void print();
}


