#include "FarmProduct.h"

using namespace std;

FarmProduct::FarmProduct()
{
	mSeasonsNumber = 0;
	mSupplierNumber = 0;
	mFarmType = NULL;
}

FarmProduct::FarmProduct(int supplierNumber, int farmtype, int seasonsnumber) {
	mSupplierNumber = supplierNumber;
	mFarmType = farmtype;
	mSeasonsNumber = seasonsnumber;

}

FarmProduct::~FarmProduct() {

}

int FarmProduct::calculatePrice() {
	return (5 - mSeasonsNumber) + 5 * mSupplierNumber + mFarmType;
}

void FarmProduct::print() {
	cin << Product::print() << endl;
}

