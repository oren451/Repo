#include "CheeseProduct.h"
#include "iostream"

using namespace std;

CheeseProduct::CheeseProduct(): MilkProduct() {
	mAddition = 0;
}

CheeseProduct::~CheeseProduct() {
}

CheeseProduct::CheeseProduct(int id, int id, int shelf
		int weight, ExposureValue exposure, const string& name, int fat,
		int colorcount, int addition)
		:MilkProduct(name, id, place, weight, exposure, fat, Cheese, colorcount)
{
	mAddition = addition;
}

int CheeseProduct::calculatePrice() {

	return MilkProduct::calculatePrice() + mAddition;
}

void CheeseProduct::print() {

	MilkProduct::print();
	cout << " (" << mAddition << ")" << endl;
}

