#pragma once


using namespace std;

class MilkProduct {

private:
	char* nameOfProduct;
	int colorCount;
	int fatValue;

public:
	MilkProduct();
	virtual ~MilkProduct();
	enum MilkType { Yogurt, Cheese, MilkDrink, Other };
};

